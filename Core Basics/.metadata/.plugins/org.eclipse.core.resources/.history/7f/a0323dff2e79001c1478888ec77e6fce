package object.oriented.programming;

public class Shape {
	private String color;
	private int width;
	private int area;
	
	public void setColor(String c) {
		color=c;
	}
	public String getColor() {
		return color;
	}
	
	public void setWidth(int c) {
		width=c;
	}
	public int getWidth() {
		return width;
	}
	public double area() {
		return 0;
		
	}
			}


