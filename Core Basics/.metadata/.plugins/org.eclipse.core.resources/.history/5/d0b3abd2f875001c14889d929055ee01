package basicjava;

public class TABLEFROM2TO10 {
	public static void main(String[] args) {
		int[][] a=new int[9] [10];
		for(int i=0;i<a.length;i++)
		{System.out.println(" ");
			for (int j=0;j<a[i].length;j++)
			{
				int c=(i+2)*(j+1); 
				a[i][j]=c
				System.out.print(c+"\t");
			}
		}
		
	}

}
