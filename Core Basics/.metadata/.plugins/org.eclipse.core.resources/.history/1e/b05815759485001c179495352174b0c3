package collection.framework;

import java.util.PriorityQueue;
import java.util.Queue;

public class QueueInterfaceMethods_Add {
	public static void main(String[] args) {
		Queue q = new PriorityQueue();
		q.add(12);
		q.add(13);
		q.add(14);
		q.add(15);      // The data type should be similar
		q.add(16);
		q.add(17);
		//q.element();   // it will retrieve all but not remove the head
		q.peek();
		System.out.println(q);
		
	}

}
