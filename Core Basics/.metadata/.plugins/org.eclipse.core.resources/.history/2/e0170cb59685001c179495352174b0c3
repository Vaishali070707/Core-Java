package collection.framework;

import java.util.PriorityQueue;
import java.util.Queue;

public class QueueInterfaceMethods_Add {
	public static void main(String[] args) {
		Queue q = new PriorityQueue();
		
	
		q.add(2);
		q.add(3);
		q.add(4);      // The data type should be similar
		q.add(5);
		q.add(6);
		
		Queue q1 = new PriorityQueue();
		
		q1.add(1);
		q1.add(3);
		q1.add(4);      // The data type should be similar
		q1.add(5);
		q1.add(6);
		//q.element();   // it will retrieve all but not remove the head
		//System.out.println(q);
		//q.remove();
		
	//	q.peek();
		//System.out.println(q.peek());
		
		q.add(q1);
		System.out.println(q);
	}

}
