package object.oriented.programming;

public class Circle extends Shape {
	
	private int radius;
	public static final double PI = 3.14;
	double ar;
	public  void ar() {
		ar = PI*radius*radius;
		System.out.println("Area of Circle ="+ar);
		
		
		}
	public int getRadius() {
		return radius;
		
	}
	     public void setRadius(int radius) {
	    	 this.radius=radius;
	     }
	
		}
